<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="dog" class="com.spring.demo01.Dog"/>
    <bean id="cat" class="com.spring.demo01.Cat"/>

    <bean id="user" class="com.spring.demo01.User" autowire="byName">
        <property name="str" value="PANXIAOWEI"/>
    </bean>

    <bean id="user1" class="com.spring.demo01.User" autowire="byType">
        <property name="str" value="PANXIAOWEI"/>
    </bean>
</beans>